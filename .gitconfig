
[user]
	name = Robin Saleh-Jan
	email = robinsalehjan@users.noreply.github.com
[credential]
	helper = osxkeychain
[core]
	editor = /usr/bin/vim
	excludesfile = .gitignore_global
[push]
	default = tracking
[color]
	branch = auto
	diff = auto
	status = auto
[color "branch"]
	current = yellow reverse
	local = yellow
	remote = green
[color "diff"]
	meta = yellow bold
	frag = magenta bold
	old = red bold
	new = green bold
[color "status"]
	added = yellow
	changed = green
	untracked = cyan
[alias]
	logp = log --pretty=oneline
	logn = log --name-status
	c = checkout
	d = diff
[help]
	autocorrect = 1
[merge]
	tool = opendiff
[pull]
	rebase = true
